{
  "expo": {
    "name": "ARI list",
    "slug": "arilist",
    "privacy": "public",
    "version": "0.0.1",
    "sdkVersion": "36.0.0",
    "platforms": ["ios", "android", "web"],
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "android": {
      "package": "com.ari.ro",
      "versionCode": 1
    },
    "ios": {
      "supportsTablet": true
    }
  }
}
